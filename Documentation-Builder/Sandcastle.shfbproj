<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <PropertyGroup>
    <!-- The configuration and platform will be used to determine which assemblies to include from solution and
				 project documentation sources -->
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
    <SchemaVersion>2.0</SchemaVersion>
    <ProjectGuid>{2f3bb7a3-041d-459b-b9fd-5c3bada24305}</ProjectGuid>
    <SHFBSchemaVersion>1.9.9.0</SHFBSchemaVersion>
    <!-- AssemblyName, Name, and RootNamespace are not used by SHFB but Visual Studio adds them anyway -->
    <AssemblyName>Documentation</AssemblyName>
    <RootNamespace>Documentation</RootNamespace>
    <Name>Documentation</Name>
    <!-- SHFB properties -->
    <FrameworkVersion>.NET Framework 4.5</FrameworkVersion>
    <OutputPath>.\Help\</OutputPath>
    <HtmlHelpName>RobotDotNet</HtmlHelpName>
    <Language>en-US</Language>
    <BuildAssemblerVerbosity>OnlyWarningsAndErrors</BuildAssemblerVerbosity>
    <HelpFileFormat>HtmlHelp1, Website</HelpFileFormat>
    <IndentHtml>False</IndentHtml>
    <KeepLogFile>True</KeepLogFile>
    <DisableCodeBlockComponent>False</DisableCodeBlockComponent>
    <CppCommentsFixup>False</CppCommentsFixup>
    <CleanIntermediates>True</CleanIntermediates>
    <DocumentationSources>
      <DocumentationSource sourceFile="sources\NetworkTables.dll" xmlns="" />
      <DocumentationSource sourceFile="sources\NetworkTables.xml" xmlns="" />
      <DocumentationSource sourceFile="sources\WPILib.dll" xmlns="" />
      <DocumentationSource sourceFile="sources\WPILib.Extras.dll" xmlns="" />
      <DocumentationSource sourceFile="sources\WPILib.Extras.xml" xmlns="" />
      <DocumentationSource sourceFile="sources\WPILib.xml" xmlns="" />
    </DocumentationSources>
    <MaximumGroupParts>4</MaximumGroupParts>
    <NamespaceGrouping>True</NamespaceGrouping>
    <SyntaxFilters>C#, Visual Basic</SyntaxFilters>
    <SdkLinkTarget>Blank</SdkLinkTarget>
    <RootNamespaceTitle>RobotDotNet</RootNamespaceTitle>
    <RootNamespaceContainer>True</RootNamespaceContainer>
    <PresentationStyle>VS2013</PresentationStyle>
    <Preliminary>False</Preliminary>
    <NamingMethod>MemberName</NamingMethod>
    <HelpTitle>RobotDotNet API Documentation</HelpTitle>
    <ContentPlacement>AboveNamespaces</ContentPlacement>
    <ProjectSummary>This is where all the RobotDotNet API Documentation is located at.</ProjectSummary>
    <NamespaceSummaries>
      <NamespaceSummaryItem name="(global)" isDocumented="False" />
<NamespaceSummaryItem name="WPILib" isDocumented="True">The WPILib namespace contains all of the component classes for the library.</NamespaceSummaryItem>
<NamespaceSummaryItem name="WPILib.Buttons" isDocumented="True">The Buttons namespace contains Buttons for use with Commands.</NamespaceSummaryItem>
<NamespaceSummaryItem name="WPILib.CAN" isDocumented="True">The CAN namespace contains the exception filtering for interfacing with CAN Jaguars.</NamespaceSummaryItem>
<NamespaceSummaryItem name="WPILib.Commands" isDocumented="True">The Commands namespace contains classes for creating Commands and Subsystems.</NamespaceSummaryItem>
<NamespaceSummaryItem name="WPILib.Exceptions" isDocumented="True">The Exceptions namespace contains custom exceptions for the WPILib.</NamespaceSummaryItem>
<NamespaceSummaryItem name="WPILib.Extras" isDocumented="True">The Extras namespace contains classes not included in the officially WPILib but we think they should be.</NamespaceSummaryItem>
<NamespaceSummaryItem name="WPILib.Interfaces" isDocumented="True">The Interfaces namespace contains common interfaces used by the WPILib.</NamespaceSummaryItem>
<NamespaceSummaryItem name="WPILib.Internal" isDocumented="True">The Internal namespace contains class implementations for the RoboRIO.</NamespaceSummaryItem>
<NamespaceSummaryItem name="WPILib.LiveWindows" isDocumented="True">The LiveWindows namespace contains LiveWindow components. Note that in java this would be called LiveWindow, but C# doesnt allow a namespace to match a class.</NamespaceSummaryItem>
<NamespaceSummaryItem name="WPILib.SmartDashboards" isDocumented="True">The SmartDashboard namespace contains classes for interfacing with the Smart Dashboard.</NamespaceSummaryItem>
<NamespaceSummaryItem name="WPILib (Group)" isGroup="True" isDocumented="True">This is where the API documentation for WPILib is located.</NamespaceSummaryItem>
<NamespaceSummaryItem name="NetworkTables (Group)" isGroup="True" isDocumented="True">This is where all of the API documentation for NetworkTables is located.</NamespaceSummaryItem>
<NamespaceSummaryItem name="NetworkTables" isDocumented="True">This is where the publicly facing NetworkTables funcionality is located at.</NamespaceSummaryItem></NamespaceSummaries>
  </PropertyGroup>
